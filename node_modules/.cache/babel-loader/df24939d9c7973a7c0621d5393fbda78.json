,KAAA,CAAA,aAAA,CAAA;AADA,OAAA;AAGA,WAAA,OAAA,CAAA,IAAA,CAAA,sBAAA;AACA;AAdA,GATA;AAyBA,EAAA,KAAA,EAAA;AACA,IAAA,KAAA,EAAA,YAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA,cAAA,CAAA,UAAA,EAAA,SAAA;AAEA,UAAA,EAAA,GAAA,IAAA;AACA,UAAA,UAAA,GAAA,EAAA;AACA,UAAA,SAAA,GAAA,CAAA;AACA,UAAA,UAAA,GAAA,EAAA;;AACA,UAAA,KAAA,CAAA,KAAA,CAAA,MAAA,IAAA,MAAA,EAAA;AACA,aAAA,YAAA,GAAA,KAAA,QAAA,CAAA,MAAA,CAAA,UAAA,IAAA,EAAA;AACA,cAAA,IAAA,CAAA,WAAA,GAAA,OAAA,CAAA,EAAA,CAAA,KAAA,CAAA,WAAA,EAAA,MAAA,CAAA,CAAA,EAAA;AACA,YAAA,UAAA,CAAA,IAAA,CAAA,IAAA;AACA;AACA,SAJA,CAAA;AAKA;;AACA,UAAA,KAAA,CAAA,KAAA,CAAA,MAAA,IAAA,aAAA,EAAA;AACA,aAAA,YAAA,GAAA,KAAA,eAAA,CAAA,MAAA,CAAA,UAAA,IAAA,EAAA;AACA,cAAA,IAAA,CAAA,WAAA,GAAA,OAAA,CAAA,EAAA,CAAA,KAAA,CAAA,WAAA,EAAA,MAAA,CAAA,CAAA,EAAA;AACA,YAAA,UAAA,CAAA,IAAA,CAAA,SAAA;AACA;;AACA,UAAA,SAAA;AACA,SALA,CAAA;;AAMA,aAAA,IAAA,CAAA,IAAA,UAAA,EAAA;AACA,UAAA,UAAA,CAAA,IAAA,CAAA,EAAA,CAAA,QAAA,CAAA,UAAA,CAAA,CAAA,CAAA,CAAA;AACA;AACA;;AACA,UAAA,KAAA,CAAA,KAAA,CAAA,MAAA,IAAA,SAAA,EAAA;AACA,aAAA,YAAA,GAAA,KAAA,WAAA,CAAA,MAAA,CAAA,UAAA,IAAA,EAAA;AACA,cAAA,IAAA,CAAA,WAAA,GAAA,OAAA,CAAA,EAAA,CAAA,KAAA,CAAA,WAAA,EAAA,MAAA,CAAA,CAAA,EAAA;AACA,YAAA,UAAA,CAAA,IAAA,CAAA,SAAA;AACA;;AACA,UAAA,SAAA;AACA,SALA,CAAA;;AAMA,aAAA,IAAA,CAAA,IAAA,UAAA,EAAA;AACA,UAAA,UAAA,CAAA,IAAA,CAAA,EAAA,CAAA,QAAA,CAAA,UAAA,CAAA,CAAA,CAAA,CAAA;AACA;AACA;;AACA,MAAA,KAAA,CAAA,QAAA,CAAA,WAAA,EAAA;AACA,QAAA,KAAA,EAAA;AADA,OAAA;AAIA;AAzCA;AAzBA,CAAA","sourcesContent":["<template>\r\n  <div class=\"list ui attached segment cont\">\r\n    <Dropdown id=\"Dropdown\" />\r\n    <div class=\"recherche ui category search\">\r\n      <div class=\"ui icon input\">\r\n        <input class=\"prompt\" v-model=\"query\" placeholder=\"Recherche..\" />\r\n        <i class=\"search icon\" />\r\n      </div>\r\n    </div>\r\n    <div class=\"resultat\">\r\n      <div\r\n        class=\"element gare\"\r\n        v-on:click.stop=\"afficheGare\"\r\n        v-for=\"gare in result\"\r\n        :key=\"gare[0]\"\r\n        :id=\"gare[1].get('nom')\"\r\n      >\r\n        <div>\r\n          <sui-icon size=\"huge\" name=\"train\" />\r\n        </div>\r\n        <div>\r\n          <h4>{{ gare[1].get(\"nom\") }}</h4>\r\n          <p>{{ gare[1].get(\"commune\") }}</p>\r\n          <p>{{ gare[1].get(\"departement\") }}</p>\r\n          <p>{{ $route.params.id }}</p>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport { mapState } from \"vuex\";\r\nimport store from \"../store/index.js\";\r\nimport Dropdown from \"@/components/Dropdown.vue\";\r\nexport default {\r\n  components: {\r\n    Dropdown\r\n  },\r\n  data: () => ({\r\n    query: \"\",\r\n    resultFiltre: []\r\n  }),\r\n  computed: mapState([\"result\", \"nameGare\", \"namedepartement\", \"namecommune\"]),\r\n  methods: {\r\n    afficheGare: function(event) {\r\n      this.$moment().locale(\"fr\");\r\n      store.dispatch(\"currentGare\", {\r\n        gare: event.currentTarget.id\r\n      });\r\n      store.dispatch(\"setEtatPage\", {\r\n        i: 1\r\n      });\r\n      console.log(store.state.historiqueGare);\r\n      store.dispatch(\"objetTrouve\", {\r\n        gare: event.currentTarget.id\r\n      })\r\n      this.$router.push(\"/recherche/infogare/\");\r\n    }\r\n  },\r\n  watch: {\r\n    query: function() {\r\n      console.log(document.getElementById(\"Dropdown\").innerText);\r\n      \r\n      var vm = this;\r\n      var listeQuery = [];\r\n      var indexItem = 0\r\n      var listeIndex = [];\r\n      if(store.state.filter == \"Name\"){\r\n        this.resultFiltre = this.nameGare.filter(function(item) {\r\n          if (item.toLowerCase().indexOf(vm.query.toLowerCase()) !== -1) {\r\n            listeQuery.push(item);\r\n          }\r\n        });       \r\n      }\r\n      if(store.state.filter == \"Departement\"){\r\n        this.resultFiltre = this.namedepartement.filter(function(item) {\r\n          if (item.toLowerCase().indexOf(vm.query.toLowerCase()) !== -1){\r\n            listeIndex.push(indexItem);\r\n          }\r\n          indexItem++;\r\n        })\r\n        for(var i in listeIndex){\r\n          listeQuery.push(vm.nameGare[listeIndex[i]]);\r\n        }        \r\n      }\r\n      if(store.state.filter == \"Commune\"){\r\n        this.resultFiltre = this.namecommune.filter(function(item) {\r\n          if (item.toLowerCase().indexOf(vm.query.toLowerCase()) !== -1){\r\n            listeIndex.push(indexItem);\r\n          }\r\n          indexItem++;\r\n        })\r\n        for(var y in listeIndex){\r\n          listeQuery.push(vm.nameGare[listeIndex[y]]);\r\n        }        \r\n      }\r\n      store.dispatch(\"queryGare\", {\r\n        liste: listeQuery\r\n      });\r\n\r\n    }\r\n  }\r\n};\r\n</script>\r\n<style>\r\n.gare {\r\n  height: 14vh;\r\n  background-color:rgb(238,238,229) !important;\r\n  margin-bottom: 30px;\r\n  color:rgb(44,132,203);\r\n  box-shadow:2px 2px 2px gray,\r\n  -1px -1px 2px white;\r\n  border-radius: 30px;\r\n  text-align: middle;\r\n}\r\n.cont{\r\n  background-color: rgb(170,211,223) !important;\r\n}\r\n.resultat {\r\n  display: grid;\r\n  grid-template-columns: repeat(3, 1fr);\r\n  grid-column-gap: 30px;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/components"}]}